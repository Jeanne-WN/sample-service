<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <Pattern>%t{yyyy-MM-dd HH:mm:ss.SSS} [%threadName] : %n%fullRequest Response Code: %statusCode %n</Pattern>
        </encoder>
    </appender>

    <appender-ref ref="STDOUT" />

</configuration>
